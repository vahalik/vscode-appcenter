{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "es6"
        ],
        "sourceMap": true,
        "rootDir": "./",
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strictNullChecks": false,
        "noUnusedParameters": true,
        "outDir": "./dist",
        "resolveJsonModule": true,
        "allowJs": false
    },
    "exclude": [
        "node_modules",
        ".vscode-test",
        "src/api/appcenter/generated",
        "./dist/"
    ]
}
